<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cmember">

	<select id="selectList" resultType="cmember">
		select * from c_member
	</select>

	<delete id="deleteMember">
		delete from C_Member where cMemberId=#{CMemberId}
	</delete>

  <insert id="insertCMember" parameterType="cmember">
  		insert into C_Member values(#{cMemberId},#{cId},#{cMemberPw},#{cMemberName},#{cMemberPhone},#{cMemberCPhone},#{cMemberDiv},DEFAULT,#{cMemberEmail})
  </insert>
  
  <select id="selectOne" resultType="cmember">
  		select * from c_member where cmemberid = #{cMemberId}
  </select>
  <update id="updateMember" parameterType="cmember">
  		UPDATE C_MEMBER SET CMEMBERNAME=#{cMemberName}, cMemberPhone=#{cMemberPhone}, cMemberEmail=#{cMemberEmail}, cMemberPw=#{cMemberPw} where cMemberId=#{cMemberId}
  </update>
  
  
</mapper>